doctype html
html
  head
    title= title
    link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/normalize/7.0.0/normalize.min.css')
    link(rel='stylesheet', href='/stylesheets/style.css')
  body
    script.
        async function handleClick(el, name, method) {
            const response = await fetch(`/dogs/${name}/vote`, { method });
            console.log(response.status);
            if (response.status === 200) {
                if (method === 'POST') {
                    el.nextSibling.innerHTML = parseInt(el.nextSibling.innerHTML) + 1;
                } else {
                    el.previousSibling.innerHTML = parseInt(el.previousSibling.innerHTML) - 1;
                }
            }



        }
    block content
